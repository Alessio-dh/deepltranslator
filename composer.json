{
    "name": "alessiodh/deepltranslator",
    "description": "Will translate all files inside a laravel project using the DeepL API and exports it to the requested language",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "proprietary",
    "require": {
        "guzzlehttp/guzzle": "^7.3",
        "ext-json": "*"
    },
    "require-dev": {
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Alessiodh\\Deepltranslator\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alessiodh\\Deepltranslator\\DeeplTranslatorServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Alessio De Houwer",
            "email": "alessio.dh@hotmail.com"
        }
    ]
}
